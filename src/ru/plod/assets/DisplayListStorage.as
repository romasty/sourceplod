package ru.plod.assets {	import flash.display.DisplayObjectContainer;	import ru.plod.util.DisplayObjectUtil;	/**	 * Created with IntelliJ IDEA.	 * User: Romasty	 * Date: 24.01.14	 * Time: 12:59	 * To change this template use File | Settings | File Templates.	 */	public class DisplayListStorage implements IResourceStorage {		private var _container : DisplayObjectContainer;		public function DisplayListStorage(container : DisplayObjectContainer = null) {			setContainer(container);		}		public function setContainer(container : DisplayObjectContainer) : void		{			_container = container;		}		public function get container():DisplayObjectContainer {			return _container;		}		public function getSource(key:*):Object {			return DisplayObjectUtil.getChildByName(_container, key);		}	}}